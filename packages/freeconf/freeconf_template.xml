<?xml version="1.0" encoding="UTF-8"?>
<freeconf-template>
    <container-name>freeconf-configuration</container-name>

    <container>
        <container-name>general</container-name>
        <group>default</group>
        <string>
            <entry-name>lang</entry-name>
            <properties>
                <data>lang_list</data>
            </properties>
        </string>
    </container>

    <container>
        <container-name>package</container-name>
        <group>default</group>
        <multiple>
            <primary>name</primary>
            <min>0</min>
        </multiple>
        <string>
            <entry-name>name</entry-name>
            <mandatory>yes</mandatory>
        </string>
        <string>
            <entry-name>location</entry-name>
            <mandatory>yes</mandatory>
        </string>
    </container>
</freeconf-template>
