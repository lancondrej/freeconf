<?xml version="1.0" encoding="UTF-8"?>
<freeconf_template>
    <container name="general">
        <string name="lang">
            <mandatory>yes</mandatory>
            <properties>
                <data>lang_list</data>
            </properties>
        </string>
    </container>

    <container name="packages_dir">
        <string name="location">
            <mandatory>yes</mandatory>
        <multiple>
            <min>1</min>
        </multiple>
        </string>

    </container>
</freeconf_template>
