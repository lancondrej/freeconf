<?xml version="1.0" encoding="UTF-8"?>
<freeconf_template>
    <container name="container1">
            <group>default</group>
            <number name="number1">
                <properties>
                    <min>0</min>
                    <max>111</max>
                    <step>1</step>
                    <precision>0</precision>
                    <print_sign>yes</print_sign>
                    <leading_zeros>10</leading_zeros>
                </properties>
            </number>
            <string name="string11">
                <mandatory>yes</mandatory>
                <group>extras</group>
                <properties>
                    <data strict="no">string-list</data>
                </properties>
            </string>
            <bool name="bool1">
                <mandatory>yes</mandatory>
            </bool>
            <string name="string12">
                <properties>
                    <regexp>(\w+@\w+(?:\.\w+)+)</regexp>
                </properties>
            </string>
        </container>
    <container name="container2">
            <group>default</group>
            <number name="number2">
                <properties>
                    <max>0.8</max>
                    <step>0.002</step>
                </properties>
            </number>
            <string name="string2">
                <mandatory>yes</mandatory>
                <properties>
                    <data>string-list</data>
                </properties>
            </string>
            <fuzzy name="fuzzy2">
                <group>extras</group>
                <properties>
                    <data>fuzzy-list</data>
                </properties>
            </fuzzy>
            <container name="container3">
                <group>default</group>
                <multiple>
                    <min>0</min>
                    <max>2</max>
                </multiple>
                <number name="number3">
                </number>
                <string name="string3">
                    <mandatory>yes</mandatory>
                </string>
            </container>
        </container>
    <container name="container-string">
            <string name="string1">
                <mandatory>yes</mandatory>
                <multiple>
                    <min>1</min>
                    <max>3</max>
                </multiple>
            </string>
    </container>
</freeconf_template>
