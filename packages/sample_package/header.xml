<?xml version="1.0" encoding="UTF-8"?>
<freeconf_header>
    <author>Ondřej Lanč</author>
    <default_language>cs</default_language>
    <content>
        <template>template.xml</template>
        <default_values>default.xml</default_values>
        <help>help.xml</help>
        <output>sample_package_conf.xml</output>
        <gui_template>gui_template.xml</gui_template>
        <gui_label>gui_help.xml</gui_label>
        <lists>lists.xml</lists>
    </content>
    <entry_group name="default">
        <transform>default.xsl</transform>
        <native_output>default.conf</native_output>
        <output_defalts>yes</output_defalts>
    </entry_group>
    <entry_group name="extras">
        <transform>extras.xsl</transform>
        <native_output>extras.conf</native_output>
        <output_defaults>yes</output_defaults>
    </entry_group>
</freeconf_header>
