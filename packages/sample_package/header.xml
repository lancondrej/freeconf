<?xml version="1.0" encoding="UTF-8"?>
<freeconf-header>
    <package>
        <name>Sample Package</name>
        <description>Sample package for Freeconf.</description>
        <author>Ondřej Lanč</author>
        <default-language>en</default-language>
    </package>
    <content>
        <template>template.xml</template>
        <default-values>default.xml</default-values>
        <help>help.xml</help>
        <output>sample_package_conf.xml</output>
        <gui-template>gui_template.xml</gui-template>
        <gui-label>gui_help.xml</gui-label>
        <lists>lists.xml</lists>
    </content>
    <entry-group name="default">
        <transform>None</transform>
        <native-output>default.conf</native-output>
        <output-defaults>yes</output-defaults>
    </entry-group>
    <entry-group name="extras">
        <transform>None</transform>
        <native-output>extras.conf</native-output>
        <output-defaults>yes</output-defaults>
    </entry-group>
</freeconf-header>
