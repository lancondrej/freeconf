<?xml version="1.0" encoding="UTF-8"?>
<freeconf-template>
    <container-name>main-container</container-name>

    <container>
        <container-name>prvni</container-name>
        <group>default</group>

        <number>
            <entry-name>number1</entry-name>
            <properties>
                <min>0.0</min>
                <max>111.0</max>
                <step>1.0</step>
            </properties>
        </number>
        <string>
            <entry-name>string1</entry-name>
            <mandatory>yes</mandatory>
            <group>default</group>
        </string>
        <bool>
            <entry-name>bool1</entry-name>
            <mandatory>yes</mandatory>
        </bool>
    </container>

    <container>
        <container-name>druhy</container-name>
        <group>default</group>

        <number>
            <entry-name>number2</entry-name>
            <properties>
                <min>0.0</min>
                <max>111.0</max>
                <step>1.0</step>
            </properties>
        </number>
        <string>
            <entry-name>string2</entry-name>
            <mandatory>yes</mandatory>
            <group>default</group>
            <properties>
                <data>list_test</data>
            </properties>
        </string>
    </container>


</freeconf-template>
