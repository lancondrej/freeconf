<?xml version="1.0" encoding="UTF-8" standalone="no" ?>
<section name="apache-config">
    <entry name="ServerRoot"><value>/etc/apache2</value></entry>
    <entry name="LockFile"><value>/var/lock/apache2/accept.lock</value></entry>
    <entry name="PidFile"><value>/var/run/apache2.pid</value></entry>
    <entry name="TimeOut"><value>300</value></entry>
    <entry name="KeepAlive"><value>yes</value></entry>
    <entry name="MaxKeepAliveRequests"><value>100</value></entry>
    <entry name="KeepAliveTimeout"><value>5</value></entry>
    <entry name="AccessFileName"><value>.htaccess</value></entry>
    <entry name="User"><value>www-data</value></entry>
    <entry name="Group"><value>www-data</value></entry>
    <entry name="DefaultType"><value>text/plain</value></entry>
    <entry name="HostnameLookups"><value>no</value></entry>
    <entry name="LogLevel"><value>warn</value></entry>
    <entry name="ErrorLog"><value>/var/log/apache2/error.log</value></entry>
    <entry name="ServerTokens"><value>OS</value></entry>
    <entry name="ServerSignature"><value>On</value></entry>
    <entry name="TraceEnable"><value>Off</value></entry>
    <section name="Listen">
        <entry name="Port"><value>80</value></entry>
        <entry name="Address"><value></value></entry>
    </section>
    <section name="VirtualHost">
        <entry name="Port"><value></value></entry>
        <section name="Directory">
            <section name="Options">
                <entry name="FollowSymlinks"><value>yes</value></entry>
                <entry name="ExecCGI"><value>yes</value></entry>
                <entry name="Includes"><value>yes</value></entry>
                <entry name="IncludesNOEXEC"><value>yes</value></entry>
                <entry name="Indexes"><value>yes</value></entry>
                <entry name="MultiViews"><value>no</value></entry>
                <entry name="SymLinksIfOwnerMatch"><value>yes</value></entry>
            </section>
            <section name="AllowOverride">
                <entry name="AuthConfig"><value>yes</value></entry>
                <entry name="FileInfo"><value>yes</value></entry>
                <entry name="Indexes"><value>yes</value></entry>
                <entry name="Limit"><value>yes</value></entry>
                <entry name="Options"><value>yes</value></entry>
            </section>
            <entry name="Order"><value>Deny,Allow</value></entry>
        </section>
    </section>
</section>
