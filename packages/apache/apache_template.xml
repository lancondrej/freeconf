<?xml version="1.0" encoding="UTF-8"?>
<freeconf_template>
    <string name="ServerRoot">
        <group>default</group>
        <mandatory>yes</mandatory>
        <properties>
            <regexp>[^/]$</regexp>
        </properties>
    </string>
    <string name="LockFile">
        <group>default</group>
        <active>no</active>
    </string>
    <string name="PidFile">
        <group>default</group>
    </string>
    <number name="TimeOut">
        <group>default</group>
        <properties>
            <min>0.0</min>
            <max>999999999.0</max>
            <step>1.0</step>
        </properties>
    </number>
    <bool name="KeepAlive">
        <group>default</group>
    </bool>
    <number name="MaxKeepAliveRequests">
        <group>default</group>
        <properties>
            <min>0.0</min>
            <max>999999999.0</max>
            <step>1.0</step>
        </properties>
    </number>
    <number name="KeepAliveTimeout">
          <group>default</group>
        <properties>
            <min>0.0</min>
            <max>999999999.0</max>
            <step>1.0</step>
        </properties>
    </number>
    <string name="AccessFileName">

        <group>default</group>
    </string>
    <string name="User">
        <group>default</group>
    </string>
    <string name="Group">
        <group>default</group>
    </string>
    <string name="DefaultType">
        <group>default</group>
    </string>
    <bool name="HostnameLookups">
        <group>default</group>
    </bool>
    <string name="LogLevel">
        <group>default</group>
        <properties>
            <data>apache_log_levels</data>
        </properties>
    </string>
    <string name="ErrorLog">
        
        <group>default</group>
    </string>
    <string name="ServerTokens">
        
        <group>default</group>
        <properties>
            <data>apache_server_tokens</data>
        </properties>
    </string>
    <string name="ServerSignature">
        
        <group>default</group>
        <properties>
            <data>apache_server_signature</data>
        </properties>
    </string>
    <string name="TraceEnable">
        
        <group>default</group>
        <properties>
            <data>apache_trace_enable</data>
        </properties>
    </string>
    <container name="Listen">
        
        <multiple>
            <primary>Address</primary>
        </multiple>
        <group>ports</group>
        <number name="Port">
            
            <group>ports</group>
            <properties>
                <min>0.0</min>
                <max>65535.0</max>
                <step>1.0</step>
            </properties>
        </number>
        <string name="Address">
            
            <group>ports</group>
        </string>
    </container>
    <container name="NameVirtualHost">
        
        <multiple>yes</multiple>
        <string name="Address">
            
            <group>default</group>
        </string>
        <number name="Port">
            
            <group>default</group>
            <properties>
                <min>0.0</min>
                <max>65535.0</max>
                <step>1.0</step>
            </properties>
        </number>
    </container>
    <container name="VirtualHost">
        
        <multiple>
            <primary>ServerName</primary>
        </multiple>
        <string name="Address">
            
            <group>default</group>
            <mandatory>yes</mandatory>
        </string>
        <string name="Port">
            
            <group>default</group>
        </string>
        <string name="ServerAdmin">
            
            <group>default</group>
            <mandatory>yes</mandatory>
        </string>
        <string name="ServerName">
            
            <group>default</group>
        </string>
        <container name="ServerAlias">
            
            <multiple>yes</multiple>
            <string name="Hostname">
                
                <group>default</group>
                <mandatory>yes</mandatory>
            </string>
        </container>
        <string name="DocumentRoot">
            
            <group>default</group>
        </string>
        <container name="Alias">
            
            <multiple>yes</multiple>
            <string name="URLPath">
                
                <group>default</group>
                <mandatory>yes</mandatory>
            </string>
            <string name="TargetPath">
                
                <group>default</group>
                <mandatory>yes</mandatory>
            </string>
        </container>
        <container name="Directory">
            
            <multiple>yes</multiple>
            <string name="Path">
                <group>default</group>
                <mandatory>yes</mandatory>
            </string>
            <container name="Options">
                
                <bool name="FollowSymlinks">
                    
                    <group>default</group>
                </bool>
                <bool name="ExecCGI">
                    
                    <group>default</group>
                </bool>
                <bool name="Includes">
                    
                    <group>default</group>
                </bool>
                <bool name="IncludesNOEXEC">
                    
                    <group>default</group>
                </bool>
                <bool name="Indexes">
                    <group>default</group>
                </bool>
                <bool name="MultiViews">
                    
                    <group>default</group>
                </bool>
                <bool name="SymLinksIfOwnerMatch">
                    
                    <group>default</group>
                </bool>
            </container>
            <container name="AllowOverride">
                
                <bool name="AuthConfig">
                    
                    <group>default</group>
                </bool>
                <bool name="FileInfo">
                    
                    <group>default</group>
                </bool>
                <bool name="Indexes">
                    
                    <group>default</group>
                </bool>
                <bool name="Limit">
                    
                    <group>default</group>
                </bool>
                <bool name="Options">
                    
                    <group>default</group>
                </bool>
            </container>
            <string name="Order">
                
                <group>default</group>
                <properties>
                    <data>apache_order</data>
                </properties>
            </string>
            <string name="Allow">
                
                <group>default</group>
            </string>
            <string name="Deny">
                
                <group>default</group>
            </string>
        </container>
        <string name="ErrorLog">
            
            <group>default</group>
        </string>
        <string name="LogLevel">
            
            <group>default</group>
            <properties>
                <data>apache_log_levels</data>
            </properties>
        </string>
    </container>
</freeconf_template>
