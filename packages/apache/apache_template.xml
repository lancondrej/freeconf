<?xml version="1.0" encoding="UTF-8"?>
<freeconf-template>
    <section-name>apache-config</section-name>
    <string>
        <entry-name>ServerRoot</entry-name>
        <group>default</group>
        <mandatory>yes</mandatory>
        <properties>
            <regexp>[^/]$</regexp>
        </properties>
    </string>
    <string>
        <entry-name>LockFile</entry-name>
        <group>default</group>
        <active>no</active>
    </string>
    <string>
        <entry-name>PidFile</entry-name>
        <group>default</group>
    </string>
    <number>
        <entry-name>TimeOut</entry-name>
        <group>default</group>
        <properties>
            <min>0.0</min>
            <max>999999999.0</max>
            <step>1.0</step>
        </properties>
    </number>
    <bool>
        <entry-name>KeepAlive</entry-name>
        <group>default</group>
    </bool>
    <number>
        <entry-name>MaxKeepAliveRequests</entry-name>
        <group>default</group>
        <properties>
            <min>0.0</min>
            <max>999999999.0</max>
            <step>1.0</step>
        </properties>
    </number>
    <number>
        <entry-name>KeepAliveTimeout</entry-name>
        <group>default</group>
        <properties>
            <min>0.0</min>
            <max>999999999.0</max>
            <step>1.0</step>
        </properties>
    </number>
    <string>
        <entry-name>AccessFileName</entry-name>
        <group>default</group>
    </string>
    <string>
        <entry-name>User</entry-name>
        <group>default</group>
    </string>
    <string>
        <entry-name>Group</entry-name>
        <group>default</group>
    </string>
    <string>
        <entry-name>DefaultType</entry-name>
        <group>default</group>
    </string>
    <bool>
        <entry-name>HostnameLookups</entry-name>
        <group>default</group>
    </bool>
    <string>
        <entry-name>LogLevel</entry-name>
        <group>default</group>
        <properties>
            <data>apache_log_levels</data>
        </properties>
    </string>
    <string>
        <entry-name>ErrorLog</entry-name>
        <group>default</group>
    </string>
    <string>
        <entry-name>ServerTokens</entry-name>
        <group>default</group>
        <properties>
            <data>apache_server_tokens</data>
        </properties>
    </string>
    <string>
        <entry-name>ServerSignature</entry-name>
        <group>default</group>
        <properties>
            <data>apache_server_signature</data>
        </properties>
    </string>
    <string>
        <entry-name>TraceEnable</entry-name>
        <group>default</group>
        <properties>
            <data>apache_trace_enable</data>
        </properties>
    </string>
    <section>
        <section-name>Listen</section-name>
        <multiple>yes</multiple>
        <group>ports</group>
        <number>
            <entry-name>Port</entry-name>
            <group>ports</group>
            <properties>
                <min>0.0</min>
                <max>65535.0</max>
                <step>1.0</step>
            </properties>
        </number>
        <string>
            <entry-name>Address</entry-name>
            <group>ports</group>
        </string>
    </section>
    <section>
        <section-name>NameVirtualHost</section-name>
        <multiple>yes</multiple>
        <string>
            <entry-name>Address</entry-name>
            <group>default</group>
        </string>
        <number>
            <entry-name>Port</entry-name>
            <group>default</group>
            <properties>
                <min>0.0</min>
                <max>65535.0</max>
                <step>1.0</step>
            </properties>
        </number>
    </section>
    <section>
        <section-name>VirtualHost</section-name>
        <multiple>yes</multiple>
        <string>
            <entry-name>Address</entry-name>
            <group>default</group>
            <mandatory>yes</mandatory>
        </string>
        <string>
            <entry-name>Port</entry-name>
            <group>default</group>
        </string>
        <string>
            <entry-name>ServerAdmin</entry-name>
            <group>default</group>
            <mandatory>yes</mandatory>
        </string>
        <string>
            <entry-name>ServerName</entry-name>
            <group>default</group>
        </string>
        <section>
            <section-name>ServerAlias</section-name>
            <multiple>yes</multiple>
            <string>
                <entry-name>Hostname</entry-name>
                <group>default</group>
                <mandatory>yes</mandatory>
            </string>
        </section>
        <string>
            <entry-name>DocumentRoot</entry-name>
            <group>default</group>
        </string>
        <section>
            <section-name>Alias</section-name>
            <multiple>yes</multiple>
            <string>
                <entry-name>URLPath</entry-name>
                <group>default</group>
                <mandatory>yes</mandatory>
            </string>
            <string>
                <entry-name>TargetPath</entry-name>
                <group>default</group>
                <mandatory>yes</mandatory>
            </string>
        </section>
        <section>
            <section-name>Directory</section-name>
            <multiple>yes</multiple>
            <string>
                <entry-name>Path</entry-name>
                <group>default</group>
                <mandatory>yes</mandatory>
            </string>
            <section>
                <section-name>Options</section-name>
                <bool>
                    <entry-name>FollowSymlinks</entry-name>
                    <group>default</group>
                </bool>
                <bool>
                    <entry-name>ExecCGI</entry-name>
                    <group>default</group>
                </bool>
                <bool>
                    <entry-name>Includes</entry-name>
                    <group>default</group>
                </bool>
                <bool>
                    <entry-name>IncludesNOEXEC</entry-name>
                    <group>default</group>
                </bool>
                <bool>
                    <entry-name>Indexes</entry-name>
                    <group>default</group>
                </bool>
                <bool>
                    <entry-name>MultiViews</entry-name>
                    <group>default</group>
                </bool>
                <bool>
                    <entry-name>SymLinksIfOwnerMatch</entry-name>
                    <group>default</group>
                </bool>
            </section>
            <section>
                <section-name>AllowOverride</section-name>
                <bool>
                    <entry-name>AuthConfig</entry-name>
                    <group>default</group>
                </bool>
                <bool>
                    <entry-name>FileInfo</entry-name>
                    <group>default</group>
                </bool>
                <bool>
                    <entry-name>Indexes</entry-name>
                    <group>default</group>
                </bool>
                <bool>
                    <entry-name>Limit</entry-name>
                    <group>default</group>
                </bool>
                <bool>
                    <entry-name>Options</entry-name>
                    <group>default</group>
                </bool>
            </section>
            <string>
                <entry-name>Order</entry-name>
                <group>default</group>
                <properties>
                    <data>apache_order</data>
                </properties>
            </string>
            <string>
                <entry-name>Allow</entry-name>
                <group>default</group>
            </string>
            <string>
                <entry-name>Deny</entry-name>
                <group>default</group>
            </string>
        </section>
        <string>
            <entry-name>ErrorLog</entry-name>
            <group>default</group>
        </string>
        <string>
            <entry-name>LogLevel</entry-name>
            <group>default</group>
            <properties>
                <data>apache_log_levels</data>
            </properties>
        </string>
    </section>
</freeconf-template>
