<?xml version="1.0" encoding="UTF-8"?>
<freeconf_header>
    <author></author>
    <default-language>en</default-language>

    <content>
      <template>apache_template.xml</template>
      <default_values>apache_default.xml</default_values>
      <help>apache_help.xml</help>
      <output>apache_conf.xml</output>
      <dependencies>apache_dependencies.xml</dependencies>
      <gui_template>apache_gui_template.xml</gui_template>
      <gui_label>apache_gui_help.xml</gui_label>
      <lists>apache_lists.xml</lists>
    </content>

    <entry_group name="default">
      <transform>apache_default_transform.xsl</transform>
      <native_output>apache2.conf</native_output>
      <output_defaults>yes</output_defaults>
    </entry_group>

    <entry_group name="ports">
      <transform>apache_default_transform.xsl</transform>
      <native_output>ports.conf</native_output>
      <output_defaults>yes</output_defaults>
    </entry_group>

</freeconf_header>
